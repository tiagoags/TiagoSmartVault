<?xml version="1.0" encoding="utf-8"?>
<businessObject>
	<propertyGroup>
		<property name="Id" type="int" />
		<property name="UserId" type="int" />
		<property name="Provider" type="string" />
		<property name="AccessToken" type="string" />
		<property name="RefreshToken" type="string" />
		<property name="TokenExpirationDate" type="System.DateTime" />
		<property name="TokenCreationDate" type="System.DateTime" />
		<property name="Scope" type="string" />
	</propertyGroup>
	 <script>
		CREATE TABLE UserOAuth (
			Id INTEGER PRIMARY KEY,
			UserId INTEGER,
			Provider TEXT,
			AccessToken TEXT,
			RefreshToken TEXT,
			TokenExpirationDate TEXT,
			TokenCreationDate TEXT,
			Scope TEXT
		);
	</script> 
</businessObject>